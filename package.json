{
  "name": "nats-topology-runner",
  "version": "0.3.0",
  "description": "Run a topology created with topology-runner using nats-jobs",
  "author": "GovSpend",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git://github.com/smartprocure/nats-topology-runner.git",
  "scripts": {
    "prepare": "npm run lint && npm test && npm run build",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc --declaration",
    "test": "jest",
    "lint": "eslint src/**"
  },
  "keywords": [
    "topology",
    "dag",
    "directed",
    "acyclic",
    "graph",
    "runner",
    "nats",
    "jobs"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "license": "ISC",
  "peerDependencies": {
    "nats-jobs": ">= 0.0.5",
    "topology-runner": ">= 0.3.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/lokijs": "^1.5.7",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "lokijs": "^1.5.12",
    "ms": "^2.1.3",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "lodash": "^4.17.21"
  }
}
